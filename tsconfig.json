{
  "compilerOptions": {
    "target": "es6",
    "module": "es2015",
    "outDir": "lib",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "plugins": [{
      "name": "typescript-snapshots-plugin"
    }]
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}